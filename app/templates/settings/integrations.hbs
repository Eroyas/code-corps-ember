{{title "Settings"}}

<div class="container">
  <div class="panel">
    <section>
      <aside>
        <h3>
          {{fa-icon "github"}} GitHub
        </h3>
        <p>Connect your account to GitHub.</p>
      </aside>
      <section>
        {{github-connect-state
          githubAvatarUrl=model.githubAvatarUrl
          githubId=model.githubId
          githubUsername=model.githubUsername
        }}
      </section>
    </section>
  </div>
</div>
